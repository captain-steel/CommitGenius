{
  "name": "commitgenius",
  "version": "1.1.5",
  "description": "cross-platform command-line tool to generate commit messages from local changes",
  "main": "index.js",
  "homepage": "https://github.com/wassim-azirar/CommitGenius",
  "author": "Wassim AZIRAR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wassim-azirar/CommitGenius.git"
  },
  "scripts": {
    "build": "tsc",
    "launch": "node dist/index.js",
    "launch:funny": "node dist/index.js --mood funny",
    "clean": "rm -rf node_modules && rm yarn.lock && yarn install",
    "update": "yarn upgrade-interactive --latest",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@types/node": "20.12.8",
    "axios": "1.6.8",
    "cli-progress": "^3.12.0",
    "dotenv": "16.4.5",
    "simple-git": "3.24.0",
    "typescript": "5.4.5",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/yargs": "17.0.32",
    "prettier": "3.2.5",
    "ts-node": "10.9.2"
  },
  "bin": {
    "CommitGenius": "dist/index.js"
  }
}
